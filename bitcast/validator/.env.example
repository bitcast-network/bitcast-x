NETUID=93
MECHID=1
SUBTENSOR_CHAIN_ENDPOINT=wss://entrypoint-finney.opentensor.ai:443
SUBTENSOR_NETWORK=finney
DISABLE_AUTO_UPDATE=false
WALLET_NAME=
HOTKEY_NAME=
PORT=8093

# Validator Mode Configuration
# Options: 'weight_copy' (default), 'standard', or 'discovery'
# - weight_copy: Fetches weights from reference validator (lightweight, no validation)
# - standard: Performs validation with downloaded social maps (medium resource usage)
# - discovery: Full validation with social discovery (high resource usage)
VALIDATOR_MODE=weight_copy

# Twitter API Provider Configuration
# Options: 'desearch' (default) or 'rapidapi'
# Switch manually by changing this value if needed
TWITTER_API_PROVIDER=desearch

# API Keys (required for 'validator' and 'reference_validator' modes)
# Not needed for 'weight_copier' mode
CHUTES_API_KEY=
WANDB_API_KEY=

# Twitter API Keys (required for 'standard' and 'discovery' modes)
# Desearch.ai API key (required when using 'desearch' provider)
# Format must be: dt_$YOUR_KEY
DESEARCH_API_KEY=dt_$

# RapidAPI key (optional: only required when using 'rapidapi' provider)
RAPID_API_KEY=